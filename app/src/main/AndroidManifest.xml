<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sui.memorygame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity7"
            android:label="@string/title_activity_7"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".TwelveCardsActivity"
            android:label="@string/title_activity_6"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EightCardsActivity"
            android:label="@string/title_activity_5"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SixCardsActivity" />
        <activity android:name=".FourCardsActivity" />
        <activity android:name=".LevelActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MusicService" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>